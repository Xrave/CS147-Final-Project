    <body>
		<div id="page" class="container">
		<div id="wrapper">
    	<div id="topBar">
			<div class="header">Notifications</div>
    	</div>

    	<div id="task-content">
            <div id="snapshotContainer">
                <img src="images/snapshotDivider.png" style="margin-bottom: 15px" />
                <table class="tabler"> 
                    <tr>
                        <td class="U1">Julie</td>
                        <td class="U2"><span class="UColor">50</span> Total Points<br /><span class="UColor">10</span> Tasks Completed<br /><span class="UColor">3</span> Rewards Claimed</td>
                    </tr>
                </table>
                <table class="tabler"> 
                    <tr>
                        <td class="U1">Xiaonan</td>
                        <td class="U2"><span class="UColor">100</span> Total Points<br /><span class="UColor">17</span> Tasks Completed<br /><span class="UColor">5</span> Rewards Claimed</td>
                    </tr>
                </table>
            </div>
            <div id="notificationsContainer">
                <img src="images/notificationsDivider.png" style="margin-bottom: 15px" /><br />
                {{#each task-items}}
                <div class="task-item2" id= "{{id}}">
                    <!--<span class="image">
                    <img src="images/{{image}}" class="profile-icon-black" /></span>
                    <span class="description"><div></div>
                    <p> {{description}} </p>
                    </span><div class="pts_text"><p> {{points}} pts awarded</p></div>-->

                    <span class="image">
                    <img src="http://temporarygiraffe.herokuapp.com/images/Placeholder_profile_black.png"></span>
                    <span class="description">
                        <span class="boldName">Julie</span> completed the task "Clean the dishes by 7". <span class="pointsAdded">+20pts</span>
                    </span>
                </div>
                <div class="task-item2" id= "{{id}}">
                    <!--<span class="image">
                    <img src="images/{{image}}" class="profile-icon-black" /></span>
                    <span class="description"><div></div>
                    <p> {{description}} </p>
                    </span><div class="pts_text"><p> {{points}} pts awarded</p></div>-->

                    <span class="image">
                    <img src="http://temporarygiraffe.herokuapp.com/images/Placeholder_profile_black.png"></span>
                    <span class="description">
                        <span class="boldName">Julie</span> completed the task "Clean the dishes by 7". <span class="pointsAdded">+20pts</span>
                    </span>
                </div>
                {{/each}}
            </div>

    	</div>
		</div>
    	<div id="bottomBar"> 
    		<a href="/"><img id="tasksIcon" class="nav-icons" src="images/tasksIcon.png" /></a>
    		<a href="/rewards" ><img id="rewardsIcon" class="nav-icons" src="images/rewardsIcon.png" /></a>
    		<a href="/notifications"><img id="notIcon" class="nav-icons" src="images/notIcon.png" /></a>
    		<a href="/settings"><img id="settingsIcon" class="nav-icons" src="images/settingsIcon.png" /></a>
			<span class="stretch"></span>
			<div id="active-highlight" style="left: 165px;"></div>
    	</div>
		</div>
		<script src="js/jquery-1.11.0.js"></script>
		<script src="js/bootstrap.js"></script>	
        <script src="js/notify.min.js"></script>

